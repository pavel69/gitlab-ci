- if current_user
  = content_for :title do
    %h3.project-title
      Dashboard
      .pull-right
        = render "search"

  .projects
    %p.fetch-status.light
      %i.icon-refresh.icon-spin
      Please wait while we fetch from GitLab (#{GitlabCi.config.gitlab_server.url})
  :javascript
    $.get("#{gitlab_projects_path}")
- else
  = render 'public'
